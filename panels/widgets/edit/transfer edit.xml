<panel id="stet" edit_mode="true">
    <attachment_group class="vertical_arrange_autosize"/>

    <record id="table_properties" auto_size="vertical">
        <flags id="mode" value="fill_rows, stripe_rows, printable" />
        <boolean id="auto_size_rows" value="true" />
        <boolean id="fixed_size_rows" value="false" />
        <string id="layout" value="30, -4,-3" />
        <integer id="row_spacing" value="0" />
        <list id="column_widget_properties">
            <record indx="0" alignment="left,centre_y" />
            <record indx="2" alignment="left,centre_y" auto_size="vertical" spec="text" colour="selection" />
        </list>

        <list id="column_table_properties">
            <record indx="0">
                <integer id="left" value="4" />
            </record>
            <record indx="1">
                <integer id="left" value="7" />
            </record>
            <record indx="2">
                <integer id="top" value="2" />
                <integer id="bottom" value="2" />
                <integer id="right" value="8" />
            </record>
        </list>
    </record>

    <record id="Cols" file="properties/transfer properties" />

    <list id="viewed_property_ids">
        <integer value="TrFe" />
			  <integer value="TrDt" />
        <integer value="StWC" />
        <integer value="ArFR" />
        <integer value="PoNs" />
        <integer value="PpNs" />
        <integer value="BuBP" />
    </list>
</panel>